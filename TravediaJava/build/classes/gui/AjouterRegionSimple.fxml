<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.AjouterRegionController">
   <children>
      <Pane prefHeight="454.0" prefWidth="728.0">
         <children>
            <Button fx:id="btn" layoutX="543.0" layoutY="390.0" mnemonicParsing="false" onAction="#AjouterRegion" prefHeight="40.0" prefWidth="110.0" style="-fx-blend-mode: #7c21d7; -fx-background-color: #812baa #812baa; -fx-background-radius: 30;" text="Ajouter " textFill="WHITE">
               <font>
                  <Font name="Gill Sans MT" size="15.0" />
               </font>
            </Button>
            <Label layoutX="143.0" layoutY="72.0" text="Nom :">
               <font>
                  <Font name="Century" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="nom" layoutX="143.0" layoutY="106.0" prefHeight="28.0" prefWidth="283.0" promptText="nom" />
            <Label layoutX="143.0" layoutY="164.0" text="Image : ">
               <font>
                  <Font name="Century" size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnup" graphicTextGap="5.0" layoutX="143.0" layoutY="197.0" mnemonicParsing="false" onAction="#handleuploadimage" style="-fx-background-radius: 30; -fx-background-color: #812baa #812baa;" text="Upload image" textFill="WHITE" />
            <ImageView fx:id="viewimage" fitHeight="145.0" fitWidth="283.0" layoutX="146.0" layoutY="243.0" pickOnBounds="true" preserveRatio="true" />
            <TextField fx:id="imagepath" editable="false" layoutX="276.0" layoutY="198.0" prefHeight="26.0" prefWidth="152.0" promptText="image path" />
            <Label layoutX="34.0" layoutY="14.0" text="Ajouter une Region">
               <font>
                  <Font name="Century" size="25.0" />
               </font>
            </Label>
            <Label fx:id="nomimg" layoutX="526.0" layoutY="96.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
