<Pane prefHeight="675.0" prefWidth="334.0" style="-fx-background-color: #10465c;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.UserProfileController">
   <children>
      <ImageView fitHeight="90.0" fitWidth="90.0" layoutX="120.0" layoutY="42.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/face_in_circle.png" />
         </image>
      </ImageView>
      <Label fx:id="username" layoutX="15.0" layoutY="223.0" prefHeight="43.0" prefWidth="149.0" text="  Nom" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="modifier" layoutX="63.0" layoutY="482.0" mnemonicParsing="false" onAction="#renderEditProfile" prefHeight="31.0" prefWidth="206.0" style="-fx-background-radius: 15; -fx-border-radius: 15; -fx-background-color: white; -fx-text-fill: #10465c;" text="Modifier" />
      <Button fx:id="supprimer" layoutX="62.0" layoutY="535.0" mnemonicParsing="false" onAction="#delete" prefHeight="31.0" prefWidth="206.0" style="-fx-background-radius: 15; -fx-border-radius: 15; -fx-background-color: #f5631d; -fx-text-fill: white;" text="Supprimer" />
